mixin btn (width, height, bgcolor, text, url)
	table(width=width, height=height, cellpadding="0", cellspacing="0", border="0", bgcolor=bgcolor).btn
		tr
			td(align="center", valign="middle", height=height).btn__td
				a(href=url, target="_blank", style="width: 200px; line-height: 44px;").btn__link= text